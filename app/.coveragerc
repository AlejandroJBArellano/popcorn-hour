[run]
source = "app"
omit =
    app/__init__.py
    app/config.py
    */site-packages/*
    */distutils/*
    tests/*
    run.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise NotImplementedError
    if __name__ == "__main__":
    pass
    raise ImportError
    except Exception as e:
    def self
    @abstractmethod

[html]
directory = coverage_html_report